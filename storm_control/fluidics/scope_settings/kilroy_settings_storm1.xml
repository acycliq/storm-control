<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>
  <!-- Default kilroy settings -->

  <!-- COM port of serial connection to valves -->
  <valves_com_port type="int">6</valves_com_port>	  

  <!-- COM port of serial connection to pump -->
  <pump_com_port type="int">7</pump_com_port>

  <!-- ID of Pump -->
  <pump_ID type="int">30</pump_ID>

  <!-- TCP/IP port for local communication with Dave -->
  <tcp_port type="int">9500</tcp_port>

  <!-- Number of valves to simulate (Defaults to 0) -->
  <num_simulated_valves type="int">0</num_simulated_valves>

  <!-- Simulate pump? (Defaults to False) -->
  <simulate_pump type="boolean">False</simulate_pump>

  <!-- Sets verbosity of kilroy and the classes it owns. Useful for debug -->
  <verbose type="boolean">True</verbose>

  <!-- Define location of valve commands and protocols -->
  <protocols_file type = "">default_config.xml</protocols_file>
  <commands_file type = "">default_config.xml</commands_file>

</settings>
